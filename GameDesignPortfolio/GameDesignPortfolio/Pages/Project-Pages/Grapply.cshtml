ound&lt;/span&gt;(powerGrapplingAudioClip);&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;16&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// Set grappling to active/true boolean is used for checks&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;17&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        grappling = &lt;span class=&quot;keyword&quot;&gt;true&lt;/span&gt;;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;18&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// Set the playerstate to Freeze to spot movement before the grapple happens&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;19&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        playerMovement.getFreeze = &lt;span class=&quot;keyword&quot;&gt;true&lt;/span&gt;;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;20&quot;&gt;&lt;/td&gt;&lt;td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;21&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// Set the grapplePoint position to the prediction hit point position&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;22&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        grapplePoint = predictionHit.point;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;23&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// Call executeGrapple with the grappleDelayTime as timer&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;24&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;method&quot;&gt;Invoke&lt;/span&gt;(&lt;span class=&quot;keyword&quot;&gt;nameof&lt;/span&gt;(&lt;span class=&quot;method&quot;&gt;ExecuteGrapple&lt;/span&gt;), grappleDelayTime);&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;25&quot;&gt;&lt;/td&gt;&lt;td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;26&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// Set the positionCount to 2 and Set the endpoint of the line&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;27&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        lineRenderer.positionCount = &lt;span class=&quot;numericLiteral&quot;&gt;2&lt;/span&gt;;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;28&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// Set a visible line using the lineRenderer to grapple point&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;29&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        lineRenderer.&lt;span class=&quot;method&quot;&gt;SetPosition&lt;/span&gt;(&lt;span class=&quot;numericLiteral&quot;&gt;1&lt;/span&gt;, grapplePoint);&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;30&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;    }&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;31&quot;&gt;&lt;/td&gt;&lt;td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;32&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;    &lt;span class=&quot;comment&quot;&gt;/// &amp;lt;summary&amp;gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;33&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;    &lt;span class=&quot;comment&quot;&gt;/// Method to grapple where you aim&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;34&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;    &lt;span class=&quot;comment&quot;&gt;/// &amp;lt;/summary&amp;gt;&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;35&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;    &lt;span class=&quot;keyword&quot;&gt;private void&lt;/span&gt; &lt;span class=&quot;method&quot;&gt;ExecuteGrapple&lt;/span&gt;()&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;36&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;    {&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;37&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// Make it so the player isn&amp;#39;t in the freeze state&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;38&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        playerMovement.getFreeze = &lt;span class=&quot;keyword&quot;&gt;false&lt;/span&gt;;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;39&quot;&gt;&lt;/td&gt;&lt;td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;40&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// Get the position right below the player&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;41&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;struct&quot;&gt;Vector3&lt;/span&gt; &lt;span class=&quot;localName&quot;&gt;lowestPoint&lt;/span&gt; = &lt;span class=&quot;keyword&quot;&gt;new&lt;/span&gt; &lt;span class=&quot;struct&quot;&gt;Vector3&lt;/span&gt;(transform.position.x, transform.position.y - &lt;span class=&quot;numericLiteral&quot;&gt;1f&lt;/span&gt;, transform.position.z);&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;42&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// Get the relative y position&lt;/span&gt; &lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;43&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;keyword&quot;&gt;float&lt;/span&gt; &lt;span class=&quot;localName&quot;&gt;grapplePointRelativeYPos&lt;/span&gt; = grapplePoint.y - &lt;span class=&quot;localName&quot;&gt;lowestPoint&lt;/span&gt;.y;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;44&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// Use the relative y position with the overshootYAxis to get the highest point in the arc path. overshootYAxis is a constant float.&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;45&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;keyword&quot;&gt;float&lt;/span&gt; &lt;span class=&quot;localName&quot;&gt;highestPointOnArc&lt;/span&gt; = &lt;span class=&quot;localName&quot;&gt;grapplePointRelativeYPos&lt;/span&gt; + overshootYAxis;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;46&quot;&gt;&lt;/td&gt;&lt;td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;47&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// If the grapple point relative y position is lower than 0, set the highest point on the arc to the overshootYAxis&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;48&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;control&quot;&gt;if&lt;/span&gt; (&lt;span class=&quot;localName&quot;&gt;grapplePointRelativeYPos&lt;/span&gt; &amp;lt; &lt;span class=&quot;numericLiteral&quot;&gt;0&lt;/span&gt;)&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;49&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;            &lt;span class=&quot;localName&quot;&gt;highestPointOnArc&lt;/span&gt; = overshootYAxis;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;50&quot;&gt;&lt;/td&gt;&lt;td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;51&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// Grapple to the position using the grapplepoint position (grapple prediction position)&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;52&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        playerMovement.&lt;span class=&quot;method&quot;&gt;JumpToPosition&lt;/span&gt;(grapplePoint, &lt;span class=&quot;localName&quot;&gt;highestPointOnArc&lt;/span&gt;);&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;53&quot;&gt;&lt;/td&gt;&lt;td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;54&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;comment&quot;&gt;// Call the method to stop the grapple and reset all booleans needed for the grapple&lt;/span&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;55&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;        &lt;span class=&quot;method&quot;&gt;Invoke&lt;/span&gt;(&lt;span class=&quot;keyword&quot;&gt;nameof&lt;/span&gt;(&lt;span class=&quot;class&quot;&gt;StopGrapple&lt;/span&gt;), &lt;span class=&quot;numericLiteral&quot;&gt;1f&lt;/span&gt;);&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class=&quot;line_no&quot; line_no=&quot;56&quot;&gt;&lt;/td&gt;&lt;td class=&quot;code_column&quot;&gt;    }&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;
